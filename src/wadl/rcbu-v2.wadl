   <!--********************************-->
   <!--        Update an agent         -->
   <!--********************************-->       
   <method name="PATCH" id="updateAgent">
      
      <wadl:doc xml:lang="EN" title="Update an agent"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Updates the specified agent.</para>
         <para>This operation updates the specified agent. Agents are updated with the JSON PATCH. For more information about the JSON PATCH, see <link
            xlink:href="http://tools.ietf.org/html/rfc6902">RFC6902</link>.</para>
         <note><title>Notes</title>
            <para>You can issue updates only for the following scenarios:</para>
            <itemizedlist>
               <listitem>
                  <para>The agent is reporting the state of the restore: <code>[{ "op": "replace", "path": "/state", "value": "queued" }]</code>. Valid values for <code>"value"</code> are:</para>
                  <itemizedlist spacing="compact">
                     <listitem><para>"queued"</para></listitem>
                     <listitem><para>"preparing"</para></listitem>
                     <listitem><para>"in_progress"</para></listitem>
                  </itemizedlist>
               </listitem>
               <listitem>
                  <para>The agent is reporting the results of the finished restore (see the example request in this section). Valid values for <code>"/state"</code> are:</para>
                  <itemizedlist spacing="compact">
                     <listitem><para>"completed"</para></listitem>
                     <listitem><para>"completed_with_errors"</para></listitem>
                     <listitem><para>"failed"</para></listitem>
                     <listitem><para>"stopped"</para></listitem>
                  </itemizedlist>
               </listitem>
               <listitem>
                  <para>The agent is issuing a request to stop the restore: <code>[{ "op": "replace", "path": "/state", "value": "stop_requested" }]</code>.</para>
               </listitem>
            </itemizedlist>
            <para>You can use the <code>"add"</code> and <code>"replace"</code> operations interchangeably as they are interpreted identically for these scenarios. The only paths that you cannot modify are those listed in this endpoint's description here.</para></note>
      </wadl:doc>
      
      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateRestore_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateRestore_request.json"/>
            </doc> 
            <param name="op" style="plain" path="$.op" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the <code>replace</code> or <code>add</code> operation to update the restore.
               </wadl:doc>
            </param>
            <param name="path" style="plain" path="$.path" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the path. Valid values are <code>/state</code>, <code>/started-time</code>, <code>/ended-time</code>, <code>/errors</code>, <code>/files_restores</code>, and <code>/bytes-restored</code>.      
               </wadl:doc>
            </param> 
            <param name="value" style="plain" path="$.value" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies a value related to the <code>path</code>.
               </wadl:doc>
            </param>
            <param name="count" style="plain" path="$.value.count">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">When <code>path</code> is <code>/errors</code>, specifies the number of errors that the restore can encounter.</wadl:doc>
            </param>
            <param name="reason" style="plain" path="$.value.reason">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">When <code>path</code> is <code>/errors</code> and an error is encountered, specifies the reason for the error.</wadl:doc>
            </param>
            <param name="diagnostics" style="plain" path="$.value.diagnostics">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">When <code>path</code> is <code>/errors</code> and an error is encountered, provides information about possible reason for the error.</wadl:doc>
            </param>
            <param name="list" style="plain" path="$.value.list" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies additional information for the errors.
               </wadl:doc>
            </param>
            <param name="index" style="plain" path="$.value.list.index" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the index for the error.
               </wadl:doc>
            </param>
            <param name="path" style="plain" path="$.value.list.path" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the path for the error.
               </wadl:doc>
            </param>
            <param name="type" style="plain" path="$.value.list.type" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the type of error.
               </wadl:doc>
            </param>
            <param name="exception" style="plain" path="$.value.list.exception" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies information about the exception.
               </wadl:doc>
            </param>
            <param name="code" style="plain" path="$.value.list.exception.code" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the code for the exception.
               </wadl:doc>
            </param>
            <param name="description" style="plain" path="$.value.list.exception.description" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies a description for the exception.
               </wadl:doc>
            </param><param name="details" style="plain" path="$.value.list.exception.details" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies details about the exception.
               </wadl:doc>
            </param> 
            
         </representation>
      </request>
      
      <response status="204">                
         <doc title="No Content" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateRestore_response-json-http.txt"/>
            </doc>  
         </representation>
      </response>
      &commonFaults;
   </method>

<!--******************************************************************-->
<!--        Events methods                                            -->
<!--******************************************************************--> 
<!--********************************-->
<!--       Post project events      -->
<!--********************************-->       
   <method name="POST" id="postEvents">
      <wadl:doc title="Post project events">
         <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Posts events from the API.</para>
         <para xmlns="http://docbook.org/ns/docbook">This operation results in the agent checking for events from the API periodically, based on a schedule. The operation also causes the agent to post heartbeat events periodically so clients can determine if the agent is online or offline. By setting the <code>event</code> to <code>agent_activate</code> the the request, you can then use the <code>mode</code> parameter to indicate how often the agent should post events.</para>
         <note xmlns="http://docbook.org/ns/docbook"><para>After a specified period of time, the agent falls back to a <code>mode</code> of <code>idle</code>.</para></note>
      </wadl:doc> 
      
      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postEvents_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-postEvents_request.json"/>
            </doc> 
            <param name="event" style="plain" path="$.event" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
               xml:lang="EN">For <code>event</code>, use <code>agent_activate</code> to have the client specify how often the agent should perform the post of events.
               </wadl:doc>
            </param>
            <param name="mode" style="plain" path="$.name" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the type of event collection. Valid values are <code>idle</code>, <code>active</code>, and <code>real_time</code>.
               
               <code>idle</code> specifies that the agents checks for and posts events periodically, but not frequetnly. You do not have a need to communicate with the agent immediately. Note: Testing the agent's <code>online</code> or <code>offline</code> status is not safe in this <code>mode</code>.
               <code>active</code> specifies that the agent checks for and posts events frequently.
               <code>real_time</code> specifies that the agent checks for and posts events very frequently. Typically, in this <code>mode</code>, you want to perform multiple, rapid, time-sensitive tasks with the agent, such as browsing files on the host or in a previous backup.
               
               </wadl:doc>
            </param> 
         </representation>
      </request>
      
      <response status="202">                
         <doc title="Accepted" xml:lang="EN">The request has been accepted for processing, but the processing has not been completed.</doc>
         <representation mediaType="application/json">                      
         </representation>
      </response>
      &commonFaults;
   </method>
   
<!--********************************-->
<!--       Get project events      -->
<!--********************************-->       
   <method name="GET" id="getEvents">
      <wadl:doc title="Retrieve project events">
         <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Retrieves all events for the specified project.</para>
         <para xmlns="http://docbook.org/ns/docbook">This operation returns events that are similar to those returned from GET /v2/agents/{id}/events{?marker,limit,sort_dir} except that this operation returns events for all agents in the project instead of a single agent. Additional <code>agent_activate</code> events that have been sent to the entire project instead of to a single agent are also returned.</para>
         <note xmlns="http://docbook.org/ns/docbook"><para>You should consider these events as transient because they might disappear after a minute or so. Therefore, this operation is most useful for monitoring the specified project's current activity.</para></note>
      </wadl:doc>  
      
      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getEvents_request-json-htttp.txt"/>
            </doc>
            <param name="marker" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the <code>id</code> of the last known activity. For example: 2d26bb8d-12d0-4579-bbd0-5db7216128ef</wadl:doc>
            </param>
            <param name="limit" style="query" type="xsd:integer"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the number of activities to list. The default
                  value is 100.
               </wadl:doc>
            </param>
            <param name="sort_dir" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the direction to sort the results. Valid values are <code>asc</code> and <code>desc</code>. The default value is <code>desc</code>.
               </wadl:doc>
            </param>
         </representation>
      </request>
      
      <response status="200">                
         <doc title="OK" xml:lang="EN">The request succeeded.</doc>
         <representation mediaType="application/json">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <xsdxt:code href="../wadl/samples-v2/cbu-getEvents_response-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-getEvents_response.json"/>
            </wadl:doc>            
         </representation>
      </response>
      &commonFaults;
      &getFaults;	
   </method>
   
