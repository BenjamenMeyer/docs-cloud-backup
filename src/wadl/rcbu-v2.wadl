   <!--***********************************************-->
   <!--     Move a vault from one agent to another    -->
   <!--***********************************************-->      
   <method name="POST" id="postAgentVault">      
      <wadl:doc xml:lang="EN" title="Move a vault from one agent to another agent"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Moves a vault from one agent to another agent.</para>
         <para>This operation takes the vault assigned to the source agent and assigns it to the agent specified in the request URL. Upon successful migration, the source agent is no longer assigned to the vault.</para>
         <para>A 403 is returned in any of the following error conditions:
         <itemizedlist>
            <listitem><para>Either the source or destination agent is not owned by the project.</para></listitem>
            <listitem><para>The vault is encrypted.</para></listitem>
            <listitem><para>The destination agent has already created a vault and it contains data.</para></listitem>
         </itemizedlist>
         </para>
      </wadl:doc>
      
      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentVault_request-json-http.txt"/>  
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentVault_request.json"/>
            </doc> 
            <param name="source_agent_id" style="plain" required="true" path="$.source_agent_id" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the agent whose vault you wish to move.
               </wadl:doc>
            </param>
         </representation>
      </request>
      
      <response status="204">                
         <doc title="No Content" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentVault_response-json-http.txt"/>  
            </doc>            
         </representation>         
      </response>
      &commonFaults;
   </method>   

   <!--********************************************************************-->
   <!--     Request to enable or change encryption for an agent's vault    -->
   <!--********************************************************************-->      
   <method name="POST" id="postAgentVaultEncryptionRequest">      
      <wadl:doc xml:lang="EN" title="Enable or change encryption for an agent's vault"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Enables or changes encryption for the specified agent's vault.</para>
         <para>This operation enables or changes encryption for the specified agent's vault.</para>
         <para>If encryption is already enabled for the vault, provide the <code>old_encrypted_password_hex</code> in the request. 
            Otherwise, <code>old_encrypted_password_hex</code> can be null. If either <code>old_encrypted_password_hex</code> is provided and the agent's 
            vault is not already encrypted, or if <code>old_encrypted_password_hex</code> is not provided and the agent's vault is already encrypted, a 403 response code is returned with a message in the response body.
         </para>
      </wadl:doc>
      
      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentVaultEncryptionRequest_request-json-http.txt"/>  
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentVaultEncryptionRequest_request.json"/> 
            </doc> 
            <param name="old_encrypted_password_hex" style="plain" required="true" path="$.old_encrypted_password_hex" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Old encrypted password in hex for the vault.
               </wadl:doc>
            </param>
            <param name="new_encrypted_password_hex" style="plain" required="true" path="$.new_encrypted_password_hex" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">New encrypted password in hex for the vault.
               </wadl:doc>
            </param>
         </representation>
      </request>
      
      <response status="204">                
         <doc title="No Content" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentVaultEncryptionRequest_response-json-http.txt"/>   
            </doc> 
         </representation>         
      </response>
      &commonFaults;
   </method>   
 
