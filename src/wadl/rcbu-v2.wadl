<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE application [
    <!ENTITY % common  SYSTEM "common-v2.ent">
    %common;
    
    
]>
<application xmlns="http://wadl.dev.java.net/2009/02"
             xmlns:wadl="http://wadl.dev.java.net/2009/02"
             xmlns:xlink="http://www.w3.org/1999/xlink"
             xmlns:db="http://docbook.org/ns/docbook"
             xmlns:xsdxt="http://docs.rackspacecloud.com/xsd-ext/v1.0"
             xmlns:xi="http://www.w3.org/2001/XInclude"
             xmlns:raxapi="http://docs.rackspace.com/volume/api/v1" 
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:backup2="http://docs.openstack.org/common/api/v2.0"
             xmlns:imageapi="http://docs.openstack.org/image/api/v2.0" 
             xmlns:atom="http://www.w3.org/2005/Atom"
             xmlns:csapi="http://docs.openstack.org/compute/api/v2" 
             xmlns:common="http://docs.openstack.org/common/api/v1.0"
             xmlns:rcbu="http://docs.openstack.org/database/api/v1.0">
   
<!--******************************************************************-->
<!--       All Resources                                              -->
<!--******************************************************************-->      
   <resources base="https://dfw.backup.api.rackspacecloud.com/">
      <resource id="version" path="v2.0">
         
         <resource id="project_id" path="{project_id}">
            <param name="project_id" style="template"
               type="xsd:string">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">
                  <para>The unique identifier of the project. Also referred to as the tenant ID 
                     or account ID.</para></wadl:doc>
                  </param>
                  
            <resource id="health" path="health">
               <method href="#getHealthCheck"/>              
               <resource id="dependent_system" path="{dependent_system}">
                  <param name="dependent_system" style="template" type="xsd:string"> 
                     <wadl:doc 
                        xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN"><para>The unique identifier for the dependent system.</para> </wadl:doc>
                  </param>
                  <method href="#getHealthDetails"/>  
               </resource>	
            </resource> 
            
            <resource id="ping" path="ping"> 	
               <method href="#getPing"/> 
            </resource>
            
            <resource id="activities" path="activities">
               <method href="#getProjectActivities"/>
            </resource>
            
            <resource id="agents" path="agents">
               <method href="#postAgent"/>
               <method href="#getProjectAgents"/>
               <resource id="agent_id" path="{agent_id}"> 
               <param name="id" style="template" type="xsd:string" required="true"> 
                  <wadl:doc 
                     xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN"><para>The agent ID. For example, 8f135b4f-7a69-4b8a-947f-5e80d772fd97</para> 
                  </wadl:doc>
               </param>
                  <method href="#updateAgent"/>
               </resource>
             </resource>           
            
            
            <resource id="events" path="events">
               <method href="#postEvents"/>
               <method href="#getEvents"/>
            </resource>
            
            <resource id="restores" path="restores">
               <method href="#postRestores"/>
               <method href="#getRestores"/>

               <resource id="restore_id" path="{restore_id}">
                  <param name="id" style="template" type="xsd:string" required="true"> 
                     <wadl:doc 
                        xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN"><para>The restore ID. For example, 4c166552-aee3-416f-8cba-c063090d2aa5</para> </wadl:doc>
                  </param>
                  <method href="#getRestoreDetails"/>
                  <method href="#updateRestore"/>
                  
                  <resource id="errors" path="errors">
                     <method href="#getRestoreErrors"/>
                  </resource>
                  
                  <resource id="event" path="events">
                     <method href="#getRestoreEvents"/>
                  </resource> 
               </resource>
                  
            </resource>
         </resource>
      </resource>
   </resources>


  
<!--******************************************************************-->
<!--       All methods                                                -->
<!--******************************************************************-->
<!--                                                                  -->
<!--******************************************************************-->
<!--       Health methods                                             -->
<!--******************************************************************-->
<!--*******************************************************-->
<!--      Get health check - ?? move to Admin WADL ??      -->
<!--*******************************************************-->
	<method name="GET" id="getHealthCheck">
		<wadl:doc xml:lang="EN" title="Perform health check"
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Performs a health check for all dependent systems.</para>
			<para>This operation performs a health check for all dependent systems. </para>			
		</wadl:doc>
	   
		<request>
			<representation mediaType="application/json" >
				<doc>
					<xsdxt:code href="../wadl/samples-v2/cbu-getHealthCheck_request-json-http.txt"/>
				</doc>
			</representation>
		</request>
	   
		<response status="200" xmlns="http://wadl.dev.java.net/2009/02">
		   <doc title="OK" xml:lang="EN">The request succeeded.</doc>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook"
				   xml:lang="EN" title="Example 1: Perform health check: JSON response">
					<xsdxt:code 
						href="../wadl/samples-v2/cbu-getHealthCheck1_response-json-http.txt"
					/>
					<xsdxt:code
						href="../wadl/samples-v2/cbu-getHealthCheck1_response.json"
					/>
				</wadl:doc>
				   <wadl:doc xmlns="http://docbook.org/ns/docbook"
				      xml:lang="EN" title="Example 2: Perform health check: JSON response">
					<xsdxt:code 
						href="../wadl/samples-v2/cbu-getHealthCheck2_response-json-http.txt"
					/>
					<xsdxt:code
						href="../wadl/samples-v2/cbu-getHealthCheck2_response.json"
					/>
				</wadl:doc>
			</representation>
		</response>
	   &commonFaults;
	   &getFaults;	   
	</method>	
<!--*******************************************************-->
<!--     Get health details   - ?? move to Admin WADL ??   -->
<!--*******************************************************-->
	<method name="GET" id="getHealthDetails">
		<wadl:doc xml:lang="EN" title="Get health details"
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Gets the health details for the specified dependent system. </para>
			<para>This operation shows whether the specified dependent system is online or offline. </para>
					
		</wadl:doc>
		
	   <request>
			<representation mediaType="application/json" >
				<doc>
					<xsdxt:code href="../wadl/samples-v2/cbu-getHealthDetails_request-json-http.txt"/>
				</doc>
			</representation>
		</request>
		
	   <response status="200" xmlns="http://wadl.dev.java.net/2009/02">
	      <doc title="OK" xml:lang="EN">The request succeeded.</doc>
			<representation mediaType="application/json">
			   <wadl:doc xmlns="http://docbook.org/ns/docbook"
			      xml:lang="EN" title="Example 1: Get health details: JSON response">
					<xsdxt:code
						href="../wadl/samples-v2/cbu-getHealthDetails1_response-json-http.txt"
					/>
					<xsdxt:code
						href="../wadl/samples-v2/cbu-getHealthDetails1_response.json"
					/>
			   </wadl:doc>
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN" title="Example 2: Get health details: JSON response">
					<xsdxt:code
					href="../wadl/samples-v2/cbu-getHealthDetails2_response-json-http.txt"
					/>
					<xsdxt:code
						href="../wadl/samples-v2/cbu-getHealthDetails2_response.json"
					/>
				</wadl:doc>
			</representation>
		</response>
	   &commonFaults;
	   &getFaults;	   
	</method> 
<!--*******************************************************-->
<!--               Get ping                                -->
<!--*******************************************************-->
   <method name="GET" id="getPing">
      <wadl:doc xml:lang="EN" title="Ping the application"
         xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Ping the application.</para>
         <para>This operation gives a quick indication of whether the application is running or not. It is used by a load balancer's health checker. </para>         
      </wadl:doc>
      
      <request>
         <representation mediaType="application/json" >
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getPing_request-json-http.txt"/>
            </doc>
         </representation>
      </request>
      
      <response status="204" xmlns="http://wadl.dev.java.net/2009/02">
         <doc title="No Content" xml:lang="EN">The server successfully fulfilled the request. There is no additional content to send in the response body.</doc>
         <representation mediaType="application/json">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
               xml:lang="EN">
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-getPing_response-json-http.txt"
               />
            </wadl:doc>
         </representation>
      </response>
      &commonFaults;
      &getFaults;	 
   </method>
   
<!--******************************************************************-->
<!--       Activity method                                            -->
<!--******************************************************************--> 
<!--********************************-->
<!-- List activities for project    -->
<!--********************************-->                    
         <method name="GET" id="getProjectActivities">
            <wadl:doc title="List activities for a project">
               <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Lists the activities for the project. </para>
               <para xmlns="http://docbook.org/ns/docbook">This operation lists the activities for the project. If no activities are available, the <code>activities</code>" parameter in the response is an empty array. (TODO: Add restorable to backups to indicate which are available for restore?)</para>
            </wadl:doc>
            
            <request>
               <param name="marker" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Specifies the <code>id</code> of the last known activity. For example: 2d26bb8d-12d0-4579-bbd0-5db7216128ef</wadl:doc>
               </param>
               <param name="limit" style="query" type="xsd:integer"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Specifies the number of activites to list. The default
                     value is 100.
                  </wadl:doc>
               </param>
               <param name="sort_dir" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Specifies the direction to sort the results. Valid values are <code>asc</code> and <code>desc</code>. The default value is <code>desc</code>.
                  </wadl:doc>
               </param>
               <param name="type" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Specifies the type of activities to filter. Valid values are <code>backup</code>, <code>cleanup</code>, or <code>restore</code>.
                     </wadl:doc>
               </param>
               <param name="state" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Specifies the state of the activities to filter. Valid values are any state defined by a backup, a cleanup, or a restore, such as <code>completed_with_errors</code>.
                  </wadl:doc>
               </param>
               <param name="agent_ids" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Specifies a comma-separated list of agent IDs by which to filter. For example: <code>8f135b4f-7a69-4b8a-947f-5e80d772fd97</code>,<code>6d94a88d-1c17-484b-9247-b76f731f4731</code>
                  </wadl:doc>
               </param>
               <param name="configuration_ids" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Specifies a comma-separated list of configuration IDs by which to filter. For example: <code>7c8ee069-568f-4d5a-932f-fb2af86b5fd5</code>,<code>abf89721-616e-4922-94d4-29757d6a8671</code>
                  </wadl:doc>
               </param>
               <representation mediaType="application/json" >
                  <doc>
                     <xsdxt:code href="../wadl/samples-v2/cbu-getProjectActivities_request-json-http.txt"/>
                  </doc>
               </representation>
            </request>
            
            <response status="200">                
               <doc title="OK" xml:lang="EN">The request succeeded.</doc>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">                                       
                  <xsdxt:code 
                     href="../wadl/samples-v2/cbu-getProjectActivities_response-json-http.txt"
                  />
                  <xsdxt:code
                     href="../wadl/samples-v2/cbu-getProjectActivities_response.json"
                  />
                  </wadl:doc>   
               </representation>               
            </response>
            &commonFaults;
            &getFaults;	 
         </method>
   <!--******************************************************************-->
   <!--       Agents methods                                            -->
   <!--******************************************************************--> 
   <!--********************************-->
   <!--        Register an agent       -->
   <!--********************************-->                    
   <method name="POST" id="postAgent">
      <wadl:doc title="Register an agent">
         <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Registers an agent. </para>
         <para xmlns="http://docbook.org/ns/docbook">This operation registers an agent.</para>
      </wadl:doc>
      
      <request>         
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgent_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgent_request.json"/>
            </doc>
            <param name="name" style="plain" path="$.name" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the name of the agent.
               </wadl:doc>
            </param>
            <param name="version" style="plain" path="$.version" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the version of the agent.
               </wadl:doc>
            </param>
            <param name="host" style="plain" path="$.host" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies information about the host.
               </wadl:doc>
            </param>
                     <param name="flavor" style="plain" path="$.host.flavor" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Specifies flavor for the agent.
                        </wadl:doc>
                     </param>
                     <param name="region" style="plain" path="$.host.region" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Specifies region for the agent.
                        </wadl:doc>
                     </param>
                     <param name="machine" style="plain" path="$.host.machine" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Specifies information about the machine.
                        </wadl:doc>
                     </param>
                            <param name="id" style="plain" path="$.host.machine.id" required="true">
                               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                  xml:lang="EN">Specifies the machine ID.
                               </wadl:doc>
                            </param>
                     <param name="os" style="plain" path="$.host.os" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Specifies information about the operating system.
                        </wadl:doc>
                     </param>
                            <param name="name" style="plain" path="$.host.os.name" required="true">
                               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                  xml:lang="EN">Specifies the name of the operating system.
                               </wadl:doc>
                            </param>
                           <param name="version" style="plain" path="$.host.os.version" required="true">
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Specifies the version of the operating system.
                              </wadl:doc>
                           </param>
                           <param name="architecture" style="plain" path="$.host.os.architecture" required="true">
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                  xml:lang="EN">Specifies the architecture of the operating system.
                              </wadl:doc>
                           </param>
                     <param name="fingerprint" style="plain" path="$.host.fingerprint" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Specifies a string determined by the agent.
                        </wadl:doc>
                     </param>
                     <param name="addresses" style="plain" path="$.host.addresses" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Specifies information about the addresses for the agent.
                        </wadl:doc>
                     </param> 
                              <param name="version" style="plain" path="$.host.addresses.version" required="true">
                                 <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                    xml:lang="EN">Specifies the version for the address.
                                 </wadl:doc>
                              </param> 
                              <param name="addr" style="plain" path="$.host.addresses.addr" required="true">
                                 <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                    xml:lang="EN">Specifies the the address.
                                 </wadl:doc>
                              </param>
            <param name="rsa_public_key" style="plain" path="$.rsa_public_key" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies information about the rsa public key.
               </wadl:doc>
            </param>
                  <param name="modulus" style="plain" path="$.rsa_public_key.modulus" required="true">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Specifies the modulus for the rsa public key.
                     </wadl:doc>
                  </param>
                  <param name="exponent" style="plain" path="$.rsa_public_key.exponent" required="true">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Specifies the exponent for the rsa public key.
                     </wadl:doc>
                  </param>
         </representation>
      </request>
      
      <response status="201">                
         <doc title="Created" xml:lang="EN"/>
         <representation mediaType="application/json">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">                                       
               <xsdxt:code 
                  href="../wadl/samples-v2/cbu-postAgent_response-json-http.txt"/>
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-postAgent_response.json"
               />
            </wadl:doc> 
            <param name="project_id" style="plain" path="$.project_id">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the project ID.
               </wadl:doc>
            </param>
            <param name="id" style="plain" path="$.id">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the agent ID.
               </wadl:doc>
            </param>
            <param name="password" style="plain" path="$.password">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the password for the agent.
               </wadl:doc>
            </param>
         </representation>               
      </response>
      &commonFaults;
      &getFaults;	 
   </method> 
   
   <!--********************************-->
   <!--    List agents for project     -->
   <!--********************************-->                    
   <method name="GET" id="getProjectAgents">
      <wadl:doc xmlns="http://docbook.org/ns/docbook" title="List agents for project">
         <para role="shortdesc">Lists the agents for a project. </para>
         <para>This operation lists agents for a project.</para>
         <note><para>If no agents have been created for the project, the <code>agents</code> parameter in the response is an empty array.</para></note>
      </wadl:doc>
      
      <request>         
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getProjectAgents_request-json-http.txt"/> 
            </doc>

               <param name="host_machine_id" style="query" type="xsd:string" required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies the ID of the host machine on which the agent resides. For example: 8f135b4f-7a69-4b8a-947f-5e80d772fd97
               </wadl:doc>
            </param>           
         </representation>
      </request>
      
      <response status="200">                
         <doc title="OK" xml:lang="EN"/>
         <representation mediaType="application/json">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">                                       
               <xsdxt:code 
                  href="../wadl/samples-v2/cbu-getProjectAgents_response-json-http.txt"/>
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-getProjectAgents_response.json"/>
            </wadl:doc>
            <param name="agents" style="plain" path="$.agents">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Specifies information about the agents for the project.
               </wadl:doc>
            </param>
                  <param name="project_id" style="plain" path="$.agents.project_id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Specifies the project ID.
                     </wadl:doc>
                  </param>                
                  <param name="id" style="plain" path="$.agents.id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Specifies the host machine ID.
                     </wadl:doc>
                  </param>
                  <param name="name" style="plain" path="$.agents.name">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Specifies the host machine name.
                     </wadl:doc>
                  </param>
                  <param name="version" style="plain" path="$.agents.version" >
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Specifies the version.
                     </wadl:doc>
                  </param>
                  <param name="host" style="plain" path="$.agents.host">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Specifies information about the host.
                     </wadl:doc>
                  </param>
                        <param name="flavor" style="plain" path="$.agents.host.flavor">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                               xml:lang="EN">Specifies flavor.
                            </wadl:doc>
                         </param>
                         <param name="region" style="plain" path="$.agents.host.region">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                               xml:lang="EN">Specifies the region.
                            </wadl:doc>
                         </param>
                         <param name="machine" style="plain" path="$.agents.host.machine">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                               xml:lang="EN">Specifies information about the machine.
                            </wadl:doc>
                          </param>
                                       <param name="id" style="plain" path="$.agents.host.machine.id" >
                                       <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                          xml:lang="EN">Specifies the machine ID.
                                       </wadl:doc>
                                       </param>                                   
                                       <param name="links" style="plain" path="$.agents.host.machine.links">
                                          <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                             xml:lang="EN">Specifies information about the links.
                                          </wadl:doc>
                                       </param>
                                             <param name="href" style="plain" path="$.agents.host.machine.links.href" >
                                               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                                  xml:lang="EN">Specifies the location (URI).
                                               </wadl:doc>
                                             </param>
                                             <param name="rel" style="plain" path="$.agents.host.machine.links.rel" >
                                               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                                  xml:lang="EN">Specifies how the href link provided is related to this resource_url.
                                               </wadl:doc>
                                             </param>  
                  <param name="os" style="plain" path="$.agents.host.os" >
