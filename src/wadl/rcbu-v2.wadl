   <method name="POST" id="postAgentEvents">      
      <wadl:doc xml:lang="EN" title="Post events for an agent"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Posts events for the specified agent.</para>
         <para>This operation posts events for the specified agent. Note that the operation to post events for a project (insert link to the operation) is similar. </para>
      </wadl:doc>
      
      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentEvents_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentEvents_request.json"/> 
            </doc> 
            <param name="event" style="plain" path="$.event" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">The event to post. Valid values are <code>startup</code> and <code>shutdown</code>. For the <code>startup</code> event, 
                  the agent is starting up, and the API communicates any pending work to the agent. For the <code>shutdown</code> event, the agent is shutting down.
               </wadl:doc>
            </param>                       
         </representation>
      </request>
      
      <response status="202">                
         <doc title="Accepted" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentEvents_response-json-http.txt"/>                  
            </doc>             
         </representation>         
      </response>
      &commonFaults;
   </method>

   <!--***************************************************-->
   <!--            List events for an agent               -->
   <!--***************************************************-->       
   <method name="GET" id="getAgentEvents">      
      <wadl:doc xml:lang="EN" title="List events for an agent"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Lists events for the specified agent.</para>
         <para>This operation lists all events for the specified agent. You should consider these events to be transient, and they might disappear after a minute or so. 
            Therefore, this operation is most useful for monitoring an agent's current activity. </para>
            <para>You can find additional event information in the following operation descriptions:
            <itemizedlist><para></para>
               <listitem><para>GET /v2/agents/{id}/browse-request/{request_id} (insert link to operation description</para></listitem>
               <listitem><para>GET /v2/agents/{id}/vault-encryption-request/{request_id} (insert link to operation description</para></listitem>
               <listitem><para>GET /v2/agents/{id}/vault-password-verification-request/{request_id} (insert link to operation description</para></listitem>
               <listitem><para>GET /v2/backups/{id}/browse-request/{request_id} (insert link to operation description</para></listitem>
               <listitem><para>GET /v2/backups/{id}/events (insert link to operation description</para></listitem>
               <listitem><para>GET /v2/cleanups/{id}/events (insert link to operation description</para></listitem>
               <listitem><para>GET /v2/restores/{id}/events (insert link to operation description</para></listitem>
            </itemizedlist></para>
      </wadl:doc>
      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getAgentEvents_request-json-http.txt"/> 
              
            </doc> 
            <param name="marker" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Event ID. For example, 5152883998. Only events newer than the event specified by marker are returned. Use of <code>marker</code> is 
                     most useful when continuously monitoring this endpoint for new events, such that old events are not repeated back to you in subsequent calls.</wadl:doc>
            </param>
            <param name="limit" style="query" type="xsd:integer"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Number of events to list. The default value is 100.
               </wadl:doc>
            </param>
            <param name="sort_dir" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Direction to sort the results. Valid values are <code>asc</code> and <code>desc</code>. The default value is <code>desc</code>.
               </wadl:doc>
            </param>
         </representation>
      </request>
      
      <response status="200">                
         <doc title="OK" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getAgentEvents_response-json-http.txt"/>  
               <xsdxt:code href="../wadl/samples-v2/cbu-getAgentEvents_response.json"/> 
            </doc>
            <param name="events" style="plain" path="$.events">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about events for the agent.
               </wadl:doc>
            </param>
                  <param name="id" style="plain" path="$.events.id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">ID for the event.
                     </wadl:doc>
                  </param>
                  <param name="time" style="plain" path="$.events.time">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Time of the event.
                     </wadl:doc>
                  </param>
                  <param name="event" style="plain" path="$.events.event">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Type of the event.
                     </wadl:doc>
                  </param>
                  <param name="agent" style="plain" path="$.events.agent">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Information about the agent for each <code>event</code> except <code>mode</code>.
                     </wadl:doc>
                  </param>
                         <param name="id" style="plain" path="$.events.agent.id">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                               xml:lang="EN">Agent ID.
                            </wadl:doc>
                         </param>
                         <param name="name" style="plain" path="$.events.agent.name">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                               xml:lang="EN">Name of the agent.
                            </wadl:doc>
                         </param>
                         <param name="host" style="plain" path="$.events.agent.host">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                               xml:lang="EN">Information about the host.
                            </wadl:doc>
                         </param>
                                 <param name="os" style="plain" path="$.events.agent.host.os">
                                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                       xml:lang="EN">Information about the operating system for the host.
                                    </wadl:doc>
                                 </param>
                                        <param name="name" style="plain" path="$.events.agent.host.os.name">
                                           <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                              xml:lang="EN">Name of the operating system.
                                           </wadl:doc>
                                        </param>
                                        <param name="version" style="plain" path="$.events.agent.host.os.version">
                                           <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                              xml:lang="EN">Version of the operating system.
                                           </wadl:doc>
                                        </param>
                                        <param name="architecture" style="plain" path="$.events.agent.host.os.architecture">
                                           <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                              xml:lang="EN">Architecture of the operating system.
                                           </wadl:doc>
                                        </param>
                  <param name="mode" style="plain" path="$.events.mode">
                      <wadl:doc xmlns="http://docbook.org/ns/docbook"
                          xml:lang="EN">Mode of the event when the <code>event</code> is <code>agent_activiate</code>.
                      </wadl:doc>
                  </param>
                  <param name="request_id" style="plain" path="$.events.request_id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">ID of the request when the <code>event</code> is <code>vault_encryption_enable</code>, <code>vault_encryption_change</code>, <code>vault_password_verify</code>, <code>vault_db_download_in_progress</code>, <code>vault_db_download_completed</code>, <code>vault_db_download_failed</code>, <code>host_browse</code>, <code>logfile_upload</code>, <code>logfile_upload</code>, or <code>logfile_completed</code> .
                     </wadl:doc>
                  </param>
                  <param name="old_encrypted_password_hex" style="plain" path="$.events.old_encrypted_password_hex">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Encrypted old password when <code>event</code> is <code>vault_encryption_change</code>.
                     </wadl:doc>
                  </param>
                  <param name="new_encrypted_password_hex" style="plain" path="$.events.new_encrypted_password_hex">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Encrypted new password when <code>event</code> is <code>vault_encryption_change</code>.
                     </wadl:doc>
