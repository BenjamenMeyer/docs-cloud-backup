   <!--*********************************************-->
   <!--        List activities for an agent         -->
   <!--*********************************************-->       
   <method name="GET" id="getAgentActivities">      
      <wadl:doc xml:lang="EN" title="List activities for an agent"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Lists the activities for the specified agent.</para>
         <para>This operation lists the activities for the specified agent. The activities include: 
            <itemizedlist>
               <listitem><para>Backups performed by the specified agent</para></listitem>
               <listitem><para>Restores performed when the specified agent is the destination</para></listitem>
               <listitem><para>Cleanups performed by the specified agent</para></listitem>
            </itemizedlist>
         </para>
         <para>If no activities are available, the <code>activities</code> parameter in the response is an empty array.</para>
      </wadl:doc>
      
      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getAgentActivities_request-json-http.txt"/> 
            </doc>
            <param name="marker" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the last known activity. For example: 2d26bb8d-12d0-4579-bbd0-5db7216128ef</wadl:doc>
            </param>
            <param name="limit" style="query" type="xsd:integer"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Number of activities to list. The default
                  value is 100.
               </wadl:doc>
            </param>
            <param name="sort_dir" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Direction to sort the results. Valid values are <code>asc</code> and <code>desc</code>. The default value is <code>desc</code>.
               </wadl:doc>
            </param>
            <param name="type" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Type of activities by which to filter. Valid values are <code>backup</code>, <code>cleanup</code>, or <code>restore</code>.
               </wadl:doc>
            </param>
            <param name="state" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">State of the activities by which to filter. Valid values are any state defined by a backup, a cleanup, or a restore, such as <code>completed_with_errors</code>.
               </wadl:doc>
            </param>
            <param name="configuration_ids" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Comma-separated list of configuration IDs by which to filter. For example: <code>7c8ee069-568f-4d5a-932f-fb2af86b5fd5</code>,<code>abf89721-616e-4922-94d4-29757d6a8671</code>
               </wadl:doc>
            </param>
         </representation>
      </request>
      
      <response status="200">                
         <doc title="OK" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getAgentActivities_response-json-http.txt"/> 
               <xsdxt:code href="../wadl/samples-v2/cbu-getAgentActivities_response.json"/> 
            </doc>
            <param name="activities" style="plain" path="$.activities">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the activities for the agent.
               </wadl:doc>
            </param>
                  <param name="id" style="plain" path="$.activities.id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">ID of the last know activity.
                     </wadl:doc>
                  </param>
                  <param name="type" style="plain" path="$.activities.type">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Type of the activity.
                     </wadl:doc>
                  </param>
                  <param name="time" style="plain" path="$.activities.time">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Time of the activity.
                     </wadl:doc>
                  </param>
                  <param name="agent" style="plain" path="$.activities.agent">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Information about the agent associated with the activity.
                     </wadl:doc>
                  </param>
                        <param name="id" style="plain" path="$.activities.agent.id">
                           <wadl:doc xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN">ID for the agent associated with the activity.
                           </wadl:doc>
                        </param>
                        <param name="links" style="plain" path="$.activities.agent.links">
                           <wadl:doc xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN">Information about the links for the agent associated with the activity.
                           </wadl:doc>
                        </param>
                               <param name="href" style="plain" path="$.activities.agent.links.href" >
                                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                     xml:lang="EN">Location (URI).
                                  </wadl:doc>
                               </param>
                               <param name="rel" style="plain" path="$.activities.agent.links.rel" >
                                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                     xml:lang="EN">How the href link provided is related to this resource_url.
                                  </wadl:doc>
                               </param>
                  <param name="configuration" style="plain" path="$.activities.configuration">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Information about the configuration associated with the activity.
                     </wadl:doc>
                  </param>
                         <param name="id" style="plain" path="$.activities.configuration.id">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                               xml:lang="EN">ID for the configuration associated with the activity.
                            </wadl:doc>
                         </param>
                         <param name="links" style="plain" path="$.activities.configuration.links">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                               xml:lang="EN">Information about the links for the configuration associated with the activity.
                            </wadl:doc>
                         </param>
                                 <param name="href" style="plain" path="$.activities.configuration.links.href" >
                                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                       xml:lang="EN">Location (URI).
                                    </wadl:doc>
                                 </param>
                                 <param name="rel" style="plain" path="$.activities.configuration.links.rel" >
                                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                       xml:lang="EN">How the href link provided is related to this resource_url.
                                    </wadl:doc>
                                 </param>
                  <param name="links" style="plain" path="$.activities.links">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Information about the links associated with the activity.
                     </wadl:doc>
                  </param>
                        <param name="href" style="plain" path="$.activities.links.href" >
                           <wadl:doc xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN">Location (URI).
                           </wadl:doc>
                        </param>
                        <param name="rel" style="plain" path="$.activities.links.rel" >
                           <wadl:doc xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN">How the href link provided is related to this resource_url.
                           </wadl:doc>
                        </param>
            <param name="links" style="plain" path="$.links">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the links for the <code>marker</code> of the activity.
               </wadl:doc>
            </param>
            <param name="href" style="plain" path="$.links.href" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Location (URI).
               </wadl:doc>
            </param>
            <param name="rel" style="plain" path="$.links.rel" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">How the href link provided is related to this resource_url.
               </wadl:doc>
            </param>
         </representation>         
      </response>
      &commonFaults;
   </method>   

