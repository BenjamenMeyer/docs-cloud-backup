                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Information about the system's environment.
                     </wadl:doc>
                     </param>
                           <param name="minimum_disk_space_mb" style="plain" path="$.system_preferences.environment.minimum_disk_space_mb" >
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Minimum disk space in megabytes (MB) set each for the following specified events - <code>backup</code>, <code>restore</code>, and <code>cleanup</code>.
                              </wadl:doc>
                           </param>
                  <param name="logging" style="plain" path="$.system_preferences.logging">
