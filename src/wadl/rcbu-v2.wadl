            </param>
            <param name="path" style="plain" path="$.inclusions.path" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Path to the resources to include in the restore.                  
               </wadl:doc>
            </param>
            <param name="exclusions" style="plain" path="$.exclusions" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Resources to exclude from the restore.                 
               </wadl:doc>
            </param>
            <param name="type" style="plain" path="$.exclusions.type" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Type of resources to exclude from the restore.                  
               </wadl:doc>
            </param>
            <param name="path" style="plain" path="$.exclusions.path" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Path of the resources to exclude from the restore.                  
               </wadl:doc>
            </param>
         </representation>
      </request>
      
      <response status="201">                
